{
  "name": "DFDC 機器類型",
  "description": "DFDC 機器的專屬測試流程",
  "enabled": true,
  "match_rules": {
    "gameid": ["osmbwjl"],
    "game_title_code_pattern": ["DFDC", "BWJL"]
  },
  "button_selectors": {
    "spin": "button.spin-btn",
    "bet": "button.bet-btn",
    "auto": "button.auto-btn",
    "balance": ".balance-display"
  },
  "video_detection": {
    "selector": "canvas#game-canvas",
    "threshold": {
      "black": 10,
      "transparent": 10,
      "monochrome": 5
    }
  },
  "special_config": {
    "balance_check_interval": 10,
    "spin_interval": 1.0,
    "enable_auto_spin": false
  }
}






